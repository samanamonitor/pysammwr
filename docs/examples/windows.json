[
    {
        "object_type": "check",
        "name": "cim_os",
        "command": "wmi",
        "class_name": "Win32_OperatingSystem",
        "alias": "os",
        "check_interval": 300,
        "stale_timeout": 900,
        "metrics": [
            "FreePhysicalMemory",
            "FreeSpaceInPagingFiles",
            "FreeVirtualMemory",
            "LastBootUpTime",
            "MaxNumberOfProcesses",
            "MaxProcessMemorySize",
            "NumberOfLicensedUsers",
            "NumberOfProcesses",
            "NumberOfUsers",
            "TotalVirtualMemorySize",
            "TotalVisibleMemorySize"
        ]
    },
    {
        "object_type": "check",
        "name": "cim_disk",
        "command": "wmi",
        "class_name": "Win32_LogicalDisk",
        "alias": "disk",
        "tag_properties": [ "Caption" ],
        "check_interval": 300,
        "stale_timeout": 900,
        "metrics": [ "FreeSpace", "Size" ]
    },
    {
        "object_type": "check",
        "name": "cim_perfcpu",
        "command": "wmi",
        "class_name": "Win32_PerfRawData_PerfOS_Processor",
        "alias": "perfcpu",
        "tag_properties": [ "Name" ],
        "check_interval": 60,
        "stale_timeout": 180,
        "metrics": [
            "C1TransitionsPersec",
            "C2TransitionsPersec",
            "C3TransitionsPersec",
            "DPCRate",
            "DPCsQueuedPersec",
            "InterruptsPersec",
            "PercentC1Time",
            "PercentC2Time",
            "PercentC3Time",
            "PercentDPCTime",
            "PercentIdleTime",
            "PercentInterruptTime",
            "PercentPrivilegedTime",
            "PercentProcessorTime",
            "PercentUserTime",
            "Timestamp_PerfTime",
            "Timestamp_Sys100NS",
            "Timestamp_Object"
        ]
    },
    {
        "object_type": "check",
        "name": "cim_cpu",
        "command": "wmi",
        "class_name": "Win32_PerfFormattedData_PerfOS_Processor",
        "alias": "cpu",
        "tag_properties": [ "Name" ],
        "check_interval": 60,
        "stale_timeout": 180,
        "metrics": [
            "C1TransitionsPersec",
            "C2TransitionsPersec",
            "C3TransitionsPersec",
            "DPCRate",
            "DPCsQueuedPersec",
            "InterruptsPersec",
            "PercentC1Time",
            "PercentC2Time",
            "PercentC3Time",
            "PercentDPCTime",
            "PercentIdleTime",
            "PercentInterruptTime",
            "PercentPrivilegedTime",
            "PercentProcessorTime",
            "PercentUserTime"
        ]
    },
    {
        "object_type": "check",
        "name": "cim_pagefile",
        "command": "wmi",
        "class_name": "Win32_PageFileUsage",
        "alias": "pagefile",
        "tag_properties": [ "Name" ],
        "check_interval": 60,
        "stale_timeout": 180,
        "metrics": [ "AllocatedBaseSize", "CurrentUsage", "PeakUsage" ]
    },
    {
        "object_type": "check",
        "name": "cim_perfdisk",
        "command": "wmi",
        "class_name": "Win32_PerfRawData_PerfDisk_PhysicalDisk",
        "alias": "perfdisk",
        "tag_properties": [ "Name" ],
        "check_interval": 60,
        "stale_timeout": 180,
        "metrics": [
            "AvgDiskBytesPerRead",
            "AvgDiskBytesPerRead_Base",
            "AvgDiskBytesPerTransfer",
            "AvgDiskBytesPerTransfer_Base",
            "AvgDiskBytesPerWrite",
            "AvgDiskBytesPerWrite_Base",
            "AvgDiskQueueLength",
            "AvgDiskReadQueueLength",
            "AvgDisksecPerRead",
            "AvgDisksecPerRead_Base",
            "AvgDisksecPerTransfer",
            "AvgDisksecPerTransfer_Base",
            "AvgDisksecPerWrite",
            "AvgDisksecPerWrite_Base",
            "AvgDiskWriteQueueLength",
            "CurrentDiskQueueLength",
            "DiskBytesPersec",
            "DiskReadBytesPersec",
            "DiskReadsPersec",
            "DiskTransfersPersec",
            "DiskWriteBytesPersec",
            "DiskWritesPersec",
            "PercentDiskReadTime",
            "PercentDiskReadTime_Base",
            "PercentDiskTime",
            "PercentDiskTime_Base",
            "PercentDiskWriteTime",
            "PercentDiskWriteTime_Base",
            "PercentIdleTime",
            "PercentIdleTime_Base",
            "SplitIOPerSec",
            "Timestamp_Object"
        ]
    },
    {
        "object_type": "check",
        "name": "cim_perfnic",
        "command": "wmi",
        "class_name": "Win32_PerfRawData_Tcpip_NetworkInterface",
        "alias": "perfnic",
        "tag_properties": [ "Name" ],
        "check_interval": 60,
        "stale_timeout": 180,
        "metrics": [
            "BytesReceivedPersec",
            "BytesSentPersec",
            "BytesTotalPersec",
            "CurrentBandwidth",
            "OffloadedConnections",
            "OutputQueueLength",
            "PacketsOutboundDiscarded",
            "PacketsOutboundErrors",
            "PacketsPersec",
            "PacketsReceivedDiscarded",
            "PacketsReceivedErrors",
            "PacketsReceivedNonUnicastPersec",
            "PacketsReceivedPersec",
            "PacketsReceivedUnicastPersec",
            "PacketsReceivedUnknown",
            "PacketsSentNonUnicastPersec",
            "PacketsSentPersec",
            "PacketsSentUnicastPersec",
            "TCPActiveRSCConnections",
            "TCPRSCAveragePacketSize",
            "TCPRSCCoalescedPacketsPersec",
            "TCPRSCExceptionsPersec",
            "Timestamp_Object"
        ]
    },
    {
        "object_type": "check",
        "name": "cim_perfprocess",
        "command": "wmi",
        "class_name": "Win32_PerfRawData_PerfProc_Process",
        "alias": "perfproc",
        "tag_properties": [ "Name", "IDProcess" ],
        "check_interval": 30,
        "stale_timeout": 90,
        "metrics": [
            "IODataBytesPersec",
            "IODataOperationsPersec",
            "IOOtherBytesPersec",
            "IOOtherOperationsPersec",
            "IOReadBytesPersec",
            "IOReadOperationsPersec",
            "IOWriteBytesPersec",
            "IOWriteOperationsPersec",
            "PageFaultsPersec",
            "PageFileBytes",
            "PageFileBytesPeak",
            "PercentPrivilegedTime",
            "PercentProcessorTime",
            "PercentUserTime",
            "PoolNonpagedBytes",
            "PoolPagedBytes",
            "PriorityBase",
            "PrivateBytes",
            "ThreadCount",
            "VirtualBytes",
            "VirtualBytesPeak",
            "WorkingSet",
            "WorkingSetPeak",
            "WorkingSetPrivate",
            "Timestamp_PerfTime"
        ]
    },
    {
        "object_type": "check",
        "name": "cim_perftssession",
        "command": "wmi",
        "class_name": "Win32_PerfRawData_TermService_TerminalServicesSession",
        "alias": "perftssession",
        "tag_properties": [ "Name" ],
        "check_interval": 60,
        "stale_timeout": 180,
        "metrics": [
            "HandleCount",
            "PageFaultsPersec",
            "PageFileBytes",
            "PageFileBytesPeak",
            "PercentPrivilegedTime",
            "PercentProcessorTime",
            "PercentUserTime",
            "PoolNonpagedBytes",
            "PoolPagedBytes",
            "PrivateBytes",
            "ThreadCount",
            "VirtualBytes",
            "VirtualBytesPeak",
            "WorkingSet",
            "WorkingSetPeak",
            "Timestamp_PerfTime"
        ]
    },
    {
        "object_type": "check",
        "name": "cim_certificates",
        "command": "wrcertificates",
        "alias": "certkey",
        "tag_properties": [ "sslcertkeyname" ],
        "check_interval": 60,
        "stale_timeout": 180,
        "metrics": [ "ssldaystoexpire" ]
    }
]
